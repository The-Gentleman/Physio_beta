<h3>Patient Name: <%= @patient.name %></h3>
<ul>
<% @exercises.all.map do |exercise| %>
<li>
<h4>Exercise Name:</h4>
<%= exercise.name %>

<br></br>
<%= link_to "Change this exercise", edit_exercise_path(exercise) %>
<br></br>
<%= link_to "Remove this exercise", exercise_path(exercise), method: :delete %>

</li>
<% end %>
</ul>




